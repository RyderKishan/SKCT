<?xml version="1.0" encoding="utf-8"?>

<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingLeft="16dp"
    android:paddingRight="16dp"
    android:paddingTop="16dp"
    android:paddingBottom="16dp"
    tools:context=".MainActivity">


    <ImageView
        android:id="@+id/imageView47"
        android:src="@drawable/ma"
        android:layout_width="match_parent"
        android:layout_height="210dp"
        android:layout_marginTop="40dp">
    </ImageView>


    <TextView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/textView118"
        android:text=""
        android:layout_marginTop="10dp"
        android:textSize="15sp"
        android:layout_below="@+id/imageView47"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="@string/sk"
        android:id="@+id/textView2"
        android:layout_below="@+id/textView118"
        android:layout_centerHorizontal="true"
        android:textSize="40sp"
        android:layout_marginTop="16dp"
        />
        <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="@string/ct"
        android:id="@+id/textView3"
        android:layout_below="@+id/textView2"
        android:layout_centerHorizontal="true"
        android:textSize="20sp"
        android:layout_marginTop="10dp"
        />
    <ImageView
        android:layout_width="180dp"
        android:layout_height="170dp"
        android:src="@drawable/skctlogo"
        android:id="@+id/imageView2"
        android:layout_below="@+id/textView3"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="25dp"
        android:contentDescription="" />
    <TextView
    android:layout_width="wrap_content"
    android:layout_height="match_parent"
    android:text="@string/h1"
    android:textStyle="bold"
    android:textSize="15sp"
        android:layout_marginTop="15dp"
    android:id="@+id/textView10"
    android:layout_below="@+id/imageView2"
    android:textColor="@color/colorPrimary"
    android:layout_alignParentLeft="true"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/t1"
        android:id="@+id/textView11"
        android:layout_marginTop="15dp"
        android:layout_below="@+id/textView10"
        android:layout_alignParentLeft="true"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:text="@string/h2"
        android:textStyle="bold"
        android:textSize="15sp"
        android:layout_marginTop="15dp"
        android:id="@+id/textView12"
        android:layout_below="@+id/textView11"
        android:textColor="@color/colorPrimary"
        android:layout_alignParentLeft="true"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/t2"
        android:layout_marginTop="15dp"
        android:id="@+id/textView13"
        android:layout_below="@+id/textView12"
        android:layout_alignParentLeft="true"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:text="@string/h3"
        android:textStyle="bold"
        android:textSize="15sp"
        android:layout_marginTop="15dp"
        android:id="@+id/textView14"
        android:layout_below="@+id/textView13"
        android:textColor="@color/colorPrimary"
        android:layout_alignParentLeft="true"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/t3"
        android:layout_marginTop="15dp"
        android:id="@+id/textView15"
        android:layout_below="@+id/textView14"
        android:layout_alignParentLeft="true"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/copy"
        android:layout_marginTop="15dp"
        android:id="@+id/textView94"
        android:layout_below="@+id/textView15"
        android:layout_centerHorizontal="true"/>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/quantum_ic_skip_previous_white_24"
        android:layout_marginTop="85dp"
        android:id="@+id/imageView48"
        android:layout_alignTop="@+id/imageView47"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/quantum_ic_skip_next_white_24"
        android:layout_marginTop="85dp"
        android:id="@+id/imageView49"
        android:layout_alignTop="@+id/imageView47"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />


</RelativeLayout>
</ScrollView>