<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_od_display"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.kisha.skct.OdDisplay">

    <TextView
        android:text="Hai "
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/textView134" />

    <TextView
        android:text="Anonymous"
        android:textColor="@color/colorPrimary"
        android:textSize="25sp"
        android:textStyle="bold"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView134"
        android:layout_centerHorizontal="true"
        android:id="@+id/textView135" />

    <TextView
        android:text="we have received your request. Your form is submitted successfully to your HOD. Please note the reference no!"
        android:layout_marginTop="16dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView135"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/textView136" />

    <TextView
        android:text="101102"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/colorPrimary"
        android:textSize="25sp"
        android:textStyle="bold"
        android:layout_below="@+id/textView136"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="16dp"
        android:id="@+id/textView137" />

    <Button
        android:text="Check Status Here"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:background="@color/colorPrimary"
        android:textColor="@color/white"
        android:layout_below="@+id/textView137"
        android:layout_centerHorizontal="true"
        android:id="@+id/button49" />

    <TextView
        android:text="Please do not resubmit the form! If so it will be dropped!"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button49"
        android:layout_marginTop="16dp"
        android:layout_centerHorizontal="true"
        android:id="@+id/textView138" />

    <Button
        android:text="Home"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView138"
        android:layout_marginTop="16dp"
        android:layout_centerHorizontal="true"
        android:background="@color/colorPrimary"
        android:textColor="@color/white"
        android:id="@+id/button45" />
</RelativeLayout>
