<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
    android:id="@+id/activity_ece_home"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.kisha.skct.EceHome">


    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:textSize="30sp"
        android:text="@string/ech1"
        android:textColor="@color/colorPrimary"
        android:textStyle="bold"
        android:id="@+id/textView42"/>

    <ImageView
        android:id="@+id/imageView32"
        android:src="@drawable/ea"
        android:layout_width="match_parent"
        android:layout_height="210dp"
        android:layout_below="@+id/textView42">
    </ImageView>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:text="@string/ect1"
        android:layout_marginTop="15dp"
        android:id="@+id/textView43"
        android:layout_below="@+id/imageView32"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:textSize="20sp"
        android:text="@string/ech2"
        android:textColor="@color/colorPrimary"
        android:layout_marginTop="15dp"
        android:textStyle="bold"
        android:id="@+id/textView44"
        android:layout_below="@+id/textView43"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:text="@string/ect2"
        android:layout_marginTop="15dp"
        android:id="@+id/textView45"
        android:layout_below="@+id/textView44"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:textSize="20sp"
        android:text="@string/ech3"
        android:textColor="@color/colorPrimary"
        android:layout_marginTop="15dp"
        android:textStyle="bold"
        android:id="@+id/textView46"
        android:layout_below="@+id/textView45"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:text="@string/ect3"
        android:layout_marginTop="15dp"
        android:id="@+id/textView47"
        android:layout_below="@+id/textView46"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:textSize="20sp"
        android:text="@string/ech4"
        android:textColor="@color/colorPrimary"
        android:layout_marginTop="15dp"
        android:textStyle="bold"
        android:id="@+id/textView48"
        android:layout_below="@+id/textView47"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:text="@string/ect4"
        android:layout_marginTop="15dp"
        android:id="@+id/textView49"
        android:layout_below="@+id/textView48"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:textSize="20sp"
        android:text="@string/ech5"
        android:textColor="@color/colorPrimary"
        android:layout_marginTop="15dp"
        android:textStyle="bold"
        android:id="@+id/textView50"
        android:layout_below="@+id/textView49"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:text="@string/ect5"
        android:layout_marginTop="15dp"
        android:id="@+id/textView51"
        android:layout_below="@+id/textView50"/>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/quantum_ic_skip_previous_white_36"
            android:layout_below="@+id/textView42"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginTop="100dp"
            android:layout_marginLeft="16dp"
            android:id="@+id/imageView33" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/quantum_ic_skip_next_white_36"
            android:layout_below="@+id/textView42"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="100dp"
            android:layout_marginRight="16dp"
            android:id="@+id/imageView34" />
</RelativeLayout>
</ScrollView>
